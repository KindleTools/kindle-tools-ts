{
  "name": "kindle-tools-ts",
  "version": "0.5.0",
  "description": "A robust TypeScript library to parse and process Amazon Kindle 'My Clippings.txt' files with smart merging, deduplication, and multiple export formats",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.js"
      },
      "require": {
        "types": "./dist/node/index.d.cts",
        "default": "./dist/node/index.cjs"
      }
    }
  },
  "imports": {
    "#core/*.js": "./src/core/*.ts",
    "#config/*.js": "./src/config/*.ts",
    "#importers/*.js": "./src/importers/*.ts",
    "#importers/core/*.js": "./src/importers/core/*.ts",
    "#importers/formats/*.js": "./src/importers/formats/*.ts",
    "#importers/shared/*.js": "./src/importers/shared/*.ts",
    "#exporters/*.js": "./src/exporters/*.ts",
    "#exporters/core/*.js": "./src/exporters/core/*.ts",
    "#exporters/formats/*.js": "./src/exporters/formats/*.ts",
    "#exporters/shared/*.js": "./src/exporters/shared/*.ts",
    "#schemas/*.js": "./src/schemas/*.ts",
    "#utils/*.js": "./src/utils/*.ts",
    "#utils/text/*.js": "./src/utils/text/*.ts",
    "#utils/fs/*.js": "./src/utils/fs/*.ts",
    "#utils/geo/*.js": "./src/utils/geo/*.ts",
    "#utils/system/*.js": "./src/utils/system/*.ts",
    "#app-types/*.js": "./src/types/*.ts",
    "#templates/*.js": "./src/templates/*.ts",
    "#domain/analytics/*.js": "./src/domain/analytics/*.ts",
    "#domain/core/*.js": "./src/domain/core/*.ts",
    "#domain/parsing/*.js": "./src/domain/parsing/*.ts",
    "#domain/*.js": "./src/domain/*.ts",
    "#errors": "./src/errors/index.ts",
    "#errors/*.js": "./src/errors/*.ts",
    "#ports": "./src/ports/index.ts",
    "#ports/*.js": "./src/ports/*.ts",
    "#ports/adapters/*.js": "./src/ports/adapters/*.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "gui": "vite --config tests/workbench/vite.config.ts",
    "gui:build": "vite build --config tests/workbench/vite.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "biome check .",
    "lint:eslint": "eslint src",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write . && pnpm run lint:eslint && pnpm test",
    "check:exports": "attw --pack . --ignore-rules=node10",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tests/workbench/tsconfig.json",
    "prepare": "husky",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "clean": "rimraf dist coverage .turbo",
    "ci": "turbo run build test lint check check:exports",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "kindle",
    "clippings",
    "highlights",
    "notes",
    "bookmarks",
    "parser",
    "ebook",
    "amazon",
    "my-clippings",
    "obsidian",
    "joplin",
    "readwise",
    "markdown",
    "export",
    "typescript"
  ],
  "author": "AndrÃ©s M. JimÃ©nez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KindleTools/kindle-tools-ts.git"
  },
  "homepage": "https://github.com/KindleTools/kindle-tools-ts#readme",
  "bugs": {
    "url": "https://github.com/KindleTools/kindle-tools-ts/issues"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "pnpm@10.27.0",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,json,md}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.10",
    "@changesets/cli": "^2.29.8",
    "@fast-check/vitest": "^0.2.4",
    "@types/handlebars": "^4.1.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-neverthrow": "^1.1.4",
    "fast-check": "^4.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "turbo": "^2.7.3",
    "typescript": "^5.9.3",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "zod-to-json-schema": "^3.25.1"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "fastest-levenshtein": "^1.0.16",
    "handlebars": "^4.7.8",
    "jszip": "^3.10.1",
    "neverthrow": "^8.2.0",
    "papaparse": "^5.5.3",
    "zod": "^4.3.4"
  }
}
